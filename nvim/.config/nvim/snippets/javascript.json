{
  "doc-example": {
    "body": [" ```typescript", " ${1:code}", " ```"],
    "prefix": "doc-block"
  },
  "doc-comment": { "body": "/** ${1:comment} */", "prefix": "doc" },
  "todo": { "body": "// TODO: ${1:message}", "prefix": "todo: message" },
  "time-function": {
    "body": [
      "console.time('${2:timername}', ${1:items});",
      "console.timeEnd(${1:timername});"
    ],
    "prefix": "timer"
  },
  "doc-block": {
    "body": ["/**", " * ${1:content}", " */"],
    "prefix": "doc-block"
  }
}

